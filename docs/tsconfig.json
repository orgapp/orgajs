{
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"lib": ["esnext", "DOM"],
		"moduleResolution": "node",
		"allowJs": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"allowImportingTsExtensions": true,
		"skipLibCheck": true,
		"isolatedModules": true,
		"target": "esnext",
		"module": "esnext"
	}
}
