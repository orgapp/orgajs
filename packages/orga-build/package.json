{
	"name": "orga-build",
	"version": "0.1.4",
	"description": "A simple tool that builds org-mode files into a website",
	"type": "module",
	"bin": {
		"orga-build": "./cli.js"
	},
	"scripts": {},
	"files": ["lib/", "cli.js", "index.js", "index.d.ts", "index.d.ts.map"],
	"exports": {
		".": {
			"types": "./index.d.ts",
			"import": "./index.js"
		},
		"./csr.jsx": "./lib/vite/csr.jsx"
	},
	"keywords": ["orgajs", "org-mode", "build", "website", "react"],
	"author": "Xiaoxing Hu <hi@xiaoxing.dev>",
	"license": "MIT",
	"dependencies": {
		"@orgajs/esbuild": "workspace:^",
		"@orgajs/node-loader": "workspace:^",
		"@orgajs/rollup": "workspace:*",
		"@vitejs/plugin-react": "^4.4.0",
		"esbuild": "^0.24.2",
		"express": "^5.1.0",
		"globby": "^14.1.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router": "^7.5.1",
		"rehype-katex": "^7.0.1",
		"serve-handler": "^6.1.6",
		"unist-util-visit-parents": "^6.0.1",
		"vite": "6.3.2"
	},
	"devDependencies": {
		"@orgajs/orgx": "workspace:^",
		"@types/express": "^5.0.1",
		"@types/node": "^22.13.1",
		"@types/react": "^19.0.8",
		"@types/react-dom": "^19.0.3",
		"@types/serve-handler": "^6.1.4",
		"orga": "workspace:^"
	}
}
