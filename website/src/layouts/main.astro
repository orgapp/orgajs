---
import Base from './base.astro'
import Article from './article.astro'
import Navbar from '../components/navbar.astro'
export interface Props {
  title: string;
  metadata?: Record<string, string>
}
const {title} = Astro.props.metadata || Astro.props;
---

<Base title={title}>
<Navbar slot="top"/>
<Article title={title}>
<slot/>
</Article>
</Base>
